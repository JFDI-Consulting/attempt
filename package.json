{
    "name": "@jfdi/attempt",
    "version": "1.3.3",
    "description": "Functional error handling implementations for sync & async, to replace try/catch forever",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && tsc && vite build",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "lint:scripts": "eslint . --ext .ts",
        "lint:styles": "stylelint ./**/*.{css,scss}",
        "format:scripts": "prettier . --write",
        "format:styles": "stylelint ./**/*.{css,scss} --fix",
        "format": "npm run format:scripts && npm run format:styles",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.1",
        "typescript": "^5.1.6",
        "vite": "^4.4.8",
        "vitest": "0.34.1",
        "vite-plugin-dts": "^2.3.0",
        "vite-plugin-static-copy": "^0.17.0",
        "rimraf": "^5.0.1",
        "@types/node": "^20.4.7",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "@vitest/coverage-c8": "^0.33.0",
        "ts-node": "^10.9.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JFDI-Consulting/attempt.git"
    },
    "keywords": [
        "fp",
        "functional programming",
        "error handling"
    ],
    "author": "Jon Silver",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/JFDI-Consulting/attempt/issues"
    },
    "homepage": "https://github.com/JFDI-Consulting/attempt#readme"
}
